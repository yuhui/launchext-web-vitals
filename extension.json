      "viewPath": "events/firstContentfulPaint.html"
    },
    {
      "displayName": "Interaction to Next Paint (INP)",
      "name": "interaction-to-next-paint",
      "categoryName": "Other Web Vitals",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {},
