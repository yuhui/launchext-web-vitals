  "dataElements": [
    {
      "displayName": "Attribution",
      "name": "metric-attribution",
      "categoryName": "Metric",
      "schema": {
        "$schema": "http://json-schema.org/draft-04/schema#",
        "type": "object",
        "properties": {
          "metricAttributionItem": {
            "enum": [
              "connectionTime",
              "dnsTime",
              "element",
              "elementRenderDelay",
              "eventEntry",
              "eventTarget",
              "eventTime",
              "eventType",
              "fcpEntry",
              "firstByteToFCP",
              "largestShiftEntry",
              "largestShiftSource",
              "largestShiftTarget",
              "largestShiftTime",
              "largestShiftValue",
              "lcpEntry",
              "lcpResourceEntry",
              "loadState",
              "navigationEntry",
              "requestTime",
              "resourceLoadDelay",
              "resourceLoadTime",
              "timeToFirstByte",
              "url",
              "waitingTime"
            ]
          }
        },
        "additionalProperties": false,
        "required": ["metricAttributionItem"]
      },
      "libPath": "src/lib/dataElements/metricAttribution.js",
